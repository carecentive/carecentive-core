{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    "https://w3id.org/security/suites/jws-2020/v1",
    "https://registry.lab.gaia-x.eu/development/api/trusted-shape-registry/v1/shapes/jsonld/trustframework#"
  ],
  "type": [
    "VerifiableCredential"
  ],
  "id": "{{{ credential_id }}}",
  "issuer": "{{{ issuer }}}",
  "issuanceDate": "{{{ issuance_date }}}",
  "credentialSubject": {
    "id": "{{{ subject_id }}}",
    "type": "gx:DataProductUsageContract",
    "gx:providedBy": {
        "id": "{{{ provider_cs_id }}}"
    },
    "gx:consumedBy": {
        "id": "{{{ consumer_cs_id }}}"
    },
    "gx:dataProduct": {
        "id": "{{{ data_product_description_cs_id }}}"
    },
    "gx:signers": [
        {
            "gx:participantRole": "Provider",
            "gx:mandatory": "Yes",
            "gx:legalValidity": "No"
        },
        {
            "gx:participantRole": "Consumer",
            "gx:mandatory": "Yes",
            "gx:legalValidity": "No"
        }
    ],
    "gx:termOfUsage": "{{{ data_product_terms_of_usage }}}",
    "gx:dataUsage": {
        "id": "{{{ data_usage_cs_id }}}"
    }
  },
  "proof": []
}
