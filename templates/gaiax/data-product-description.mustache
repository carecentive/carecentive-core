{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    "https://w3id.org/security/suites/jws-2020/v1",
    "https://registry.lab.gaia-x.eu/development/api/trusted-shape-registry/v1/shapes/jsonld/trustframework#",
    "https://www.w3.org/ns/dcat.jsonld"
  ],
  "type": "VerifiableCredential",
  "id": "{{{ credential_id }}}",
  "issuer": "{{{ issuer }}}",
  "issuanceDate": "{{{ issuance_date }}}",
  "credentialSubject": {
    "id": "{{{ subject_id }}}",
    "type": "gx:DataProductDescription",
    "gx:providedBy": {
      "id": "{{{ participant_cs_id }}}"
    },
    "gx:policy": "{{{ policy_contents }}}",
    "gx:termsAndConditions": {
      "gx:URL": "{{{ terms_and_conditions_url }}}",
      "gx:hash": "{{{ terms_and_conditions_hash }}}"
    },
    "gx:dataAccountExport": {
      "gx:requestType": "API",
      "gx:accessType": "digital",
      "gx:formatType": "application/json"
    },
    "gx:aggregationOf": [
        {"id": "{{{ dataset_description_cs_id }}}"}
    ],
    "dct:license": ["{{{ license_url }}}"],
    "gx:title": "{{{ data_product_title }}}",
    {{#data_product_description}}"gx:description": "{{{ data_product_description }}}",{{/data_product_description}}
    "dct:issued": "{{{ issuance_date }}}",
    {{#obsolete_date}}"gx:obsoleteDateTime": "{{{ obsolete_date }}}",{{/obsolete_date}}
    "gx:dataLicensors": [],
    "gx:dataUsageAgreement": [],
    "dct:identifier": "{{{ uuid }}}"
  }
}
